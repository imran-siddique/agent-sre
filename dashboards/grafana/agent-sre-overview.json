{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "SLO health, error budgets, cost tracking, latency, and incident monitoring for AI agents",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "SLO Health — Budget Remaining",
      "description": "Gauge showing error budget remaining as a percentage",
      "type": "gauge",
      "id": 1,
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_slos_healthy / clamp_min(agent_sre_slos_total, 1)",
          "legendFormat": "Budget Remaining",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.2, "color": "orange" },
              { "value": 0.5, "color": "yellow" },
              { "value": 0.95, "color": "green" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "Error Budget Burn Rate",
      "description": "Rate at which error budget is consumed over time",
      "type": "timeseries",
      "id": 2,
      "gridPos": { "h": 8, "w": 10, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "deriv(agent_sre_slos_total[5m]) - deriv(agent_sre_slos_healthy[5m])",
          "legendFormat": "Burn Rate",
          "refId": "A"
        },
        {
          "expr": "1 - (agent_sre_slos_healthy / clamp_min(agent_sre_slos_total, 1))",
          "legendFormat": "Budget Consumed %",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 2, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Incident Count by Severity",
      "description": "Number of open incidents",
      "type": "stat",
      "id": 3,
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_incidents_open",
          "legendFormat": "Open Incidents",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "orange" },
              { "value": 3, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "title": "Service Up",
      "type": "stat",
      "id": 4,
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_up",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "none"
      }
    },
    {
      "title": "Registered SLOs",
      "type": "stat",
      "id": 5,
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_slos_total",
          "legendFormat": "Total",
          "refId": "A"
        },
        {
          "expr": "agent_sre_slos_healthy",
          "legendFormat": "Healthy",
          "refId": "B"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "title": "API Requests Total",
      "type": "stat",
      "id": 6,
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_requests_total",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "title": "Agent Latency Percentiles",
      "description": "Request rate patterns as a proxy for agent latency distribution",
      "type": "timeseries",
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(agent_sre_requests_total[1m])",
          "legendFormat": "Request Rate /min",
          "refId": "A"
        },
        {
          "expr": "deriv(agent_sre_requests_total[5m])",
          "legendFormat": "Request Trend",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Cost per Agent (USD)",
      "description": "Organization cost spend tracked over time",
      "type": "timeseries",
      "id": 8,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_cost_org_spent_month",
          "legendFormat": "Org Spend (USD)",
          "refId": "A"
        },
        {
          "expr": "rate(agent_sre_cost_org_spent_month[5m])",
          "legendFormat": "Spend Rate",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Spend Rate" },
            "properties": [
              { "id": "custom.axisPlacement", "value": "right" },
              { "id": "unit", "value": "currencyUSD" }
            ]
          }
        ]
      }
    },
    {
      "title": "Chaos Experiment Results",
      "description": "Table of chaos experiment outcomes (populate via API)",
      "type": "table",
      "id": 9,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_up",
          "legendFormat": "Service Availability",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": []
      }
    },
    {
      "title": "Incident Timeline",
      "description": "Open incidents tracked over time",
      "type": "timeseries",
      "id": 10,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_incidents_open",
          "legendFormat": "Open Incidents",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1
          },
          "color": { "mode": "fixed", "fixedColor": "red" }
        },
        "overrides": []
      }
    },
    {
      "title": "Circuit Breaker Status",
      "description": "Derived from SLO compliance — circuit opens when compliance drops below 50%",
      "type": "state-timeline",
      "id": 11,
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "agent_sre_slos_healthy / clamp_min(agent_sre_slos_total, 1) < 0.5",
          "legendFormat": "Circuit Open",
          "refId": "A"
        },
        {
          "expr": "agent_sre_up",
          "legendFormat": "Service Reachable",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "CLOSED", "color": "green" }, "1": { "text": "OPEN", "color": "red" } } }
          ],
          "custom": {
            "fillOpacity": 80,
            "lineWidth": 0
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Service Reachable" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
                ]
              }
            ]
          }
        ]
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["agent-sre", "slo", "ai-agents", "observability"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Agent SRE Overview",
  "uid": "agent-sre-overview",
  "version": 1,
  "refresh": "15s"
}
